<li id="<%= dom_id calendar %>" class="flex items-center justify-between gap-x-6 py-5">
  <div class="min-w-0 w-full columns-2 md:columns-3">
    <%= render_text_column value: link_to(calendar.name, calendar_url(calendar)), css: "truncate mort-link-primary" do |value| %>
      <%= value %>
      <% if current_user.global_queries? %>
        <p class="text-xs"><%=  link_to calendar.account.name, calendar.account %> </p>
      <% end %>
    <% end %>
    <%= render_text_column value: calendar.time_zone, css: "hidden lg:block truncate" %>
    <%= render_datetime_column value: calendar.updated_at, css: "hidden lg:block" %>
  </div>
  <div class="flex flex-none items-center gap-x-4 ">
<!--    <a href="<%#= product_stock_items_url(product) %>" class=" rounded-md bg-white px-2.5 py-1.5 text-sm font-semibold text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 hover:bg-gray-50 sm:block">Lokationer<span class="sr-only">, GraphQL API</span></a>-->
    <%= render_contextmenu resource: calendar, resource_class: Calendar, alter: true, links: [ edit_calendar_url(calendar), calendar_url(calendar) ] %>
  </div>
</li>

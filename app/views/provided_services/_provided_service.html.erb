<li id="<%= dom_id provided_service %>" class="flex items-center justify-between gap-x-6 py-5">
  <div class="min-w-0 w-full columns-2 md:columns-4">
    <%= render_text_column value: show_resource_link(resource: provided_service), css: "truncate mort-link-primary" do |value| %>
      <%= value %>
      <% if global_queries? Current.user %>
        <p class="text-xs"><%= show_resource_link(resource: provided_service.tenant) %> </p>
      <% end %>
    <% end %>
    <%= render_text_column value: provided_service.service, css: "truncate" %>
    <%= render_text_column value: provided_service.authorized?, css: "hidden md:block truncate" do |value| %>
      <%= value ? 
            I18n.t('yes') :
            render(partial: "provided_services/#{provided_service.service.underscore}/authorize", locals: { path: request.path })
             %>
    <% end %>
    <%= render_datetime_column value: provided_service.updated_at, css: "hidden md:block" %>
  </div>
  <div class="flex flex-none items-center gap-x-4 ">
<!--    <a href="<%#= product_stock_items_url(product) %>" class=" rounded-md bg-white px-2.5 py-1.5 text-sm font-semibold text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 hover:bg-gray-50 sm:block">Lokationer<span class="sr-only">, GraphQL API</span></a>-->
    <%= render_contextmenu resource: provided_service, turbo_frame: "form", resource_class: ProvidedService, alter: true, links: [ edit_provided_service_url(provided_service), provided_service_url(provided_service) ] %>
  </div>
</li>

<tr class="gap-x-6 py-5">
  <%= render_text_column    value: link_to(tenant.name, tenant_url(tenant)), table: :row, css: "truncate mort-link-primary" %>
  <%= render_text_column    value: tenant.locale, table: :row, css: "text-right" %>

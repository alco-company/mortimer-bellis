    <%= render_text_column value: project.end_date, table: :row, css: "truncate" %>
    <%= render_text_column value: project.state, table: :row, css: "truncate" %>
    <%= render_text_column value: project.is_billable, table: :row, css: "truncate" %>
    <%= render_text_column value: project.priority, table: :row, css: "truncate" %>
    <%= render_text_column value: project.hourly_rate, table: :row, css: "truncate" %>

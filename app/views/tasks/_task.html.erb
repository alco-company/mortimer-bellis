<div id="<%= dom_id task %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Tenant:</strong>
    <%= task.tenant_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Title:</strong>
    <%= task.title %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Description:</strong>
    <%= task.description %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">State:</strong>
    <%= task.state %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Priority:</strong>
    <%= task.priority %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Due at:</strong>
    <%= task.due_at %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Completed at:</strong>
    <%= task.completed_at %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Ancestry:</strong>
    <%= task.ancestry %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Project:</strong>
    <%= task.project_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Location:</strong>
    <%= task.location_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Progress:</strong>
    <%= task.progress %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Archived:</strong>
    <%= task.archived %>
  </p>

</div>

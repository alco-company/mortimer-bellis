<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= button_to I18n.t("devise.sign_in_with", provider: OmniAuth::Utils.camelize(provider)), 
      omniauth_authorize_path(resource_name, provider), 
      data: { turbo: false }, 
      class: "mort-btn-primary" %><br />
  <% end %>
  <hr class="my-2">
<% end if controller_name == 'sessions' %>

<%= link_to I18n.t("need_help"), help_pages_url(locale: I18n.locale), class: "mort-link-primary" %>
<hr class="my-2">
<% params[:locale]="da" %>
<%= link_to I18n.t('danish'), url_for(params.permit!), class: "mort-link-primary" , data: { turbo_frame: "_top"} if I18n.locale != :da %>
<% params[:locale]="en" %>
<%= link_to I18n.t('english'), url_for(params.permit!), class: "mort-link-primary" , data: { turbo_frame: "_top"} if I18n.locale != :en %>
<% params[:locale]=I18n.locale %>
<br>
